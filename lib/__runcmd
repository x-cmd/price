
___x_cmd_price___runmain(){
    ___x_cmd_price___runcmd "$@"
}

___x_cmd_price___runcmd(){
    if ___x_cmd_hascmd price; then
        ___x_cmd_price___runcmd(){  ___x_cmd_cmds price "$@"; }
    else
        ___x_cmd_price___install || return
        ___x_cmd_price___runcmd(){ "$___X_CMD_price_BIN_PATH/$___X_CMD_price_BIN" "$@" ; }
    fi || return
    ___x_cmd_price___runcmd "$@"
}
