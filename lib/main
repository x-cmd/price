# shellcheck shell=dash

xrc:mod:lib     price   __runcmd

___x_cmd_price___main(){
    [ "$#" -gt 0 ] ||   set -- --help

    local op="$1";      shift
    case "$op" in
        -h|--help)      ___x_cmd help -m price "$@";    return 0 ;;

        *)              N=price M="Unknown subcmd -> $op" log:ret:64 ;;
    esac
}
